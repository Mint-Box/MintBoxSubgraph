{
	"name": "MintBoxSubgraph",
	"license": "UNLICENSED",
	"scripts": {
		"codegen": "graph codegen --output-dir src/types/",
		"build": "graph build",
		"deploy:ethereum": "yarn prepare:ethereum && yarn ethereum",
		"deploy:matic": "yarn prepare:matic && yarn matic",
		"deploy:bsc": "yarn prepare:bsc && yarn bsc",
		"deploy:rinkeby": "yarn prepare:rinkeby && yarn rinkeby",
		"deploy:mumbai": "yarn prepare:mumbai && yarn mumbai",
		"deploy:chapel": "yarn prepare:chapel && yarn chapel",
		"ethereum": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/ethereummintBox",
		"matic": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/maticmintbox",
		"bsc": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/bscmintbox",
		"mumbai": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/mints-on-mumbai",
		"rinkeby": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/mints-on-rinkeby",
		"chapel": "graph deploy --node https://api.thegraph.com/deploy/ dinn2018/mints-on-chapel",
		"prepare:ethereum": "mustache config/ethereum.json subgraph.template.yaml > subgraph.yaml",
		"prepare:matic": "mustache config/matic.json subgraph.template.yaml > subgraph.yaml",
		"prepare:bsc": "mustache config/bsc.json subgraph.template.yaml > subgraph.yaml",
		"prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
		"prepare:mumbai": "mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml",
		"prepare:chapel": "mustache config/chapel.json subgraph.template.yaml > subgraph.yaml",
		"create-local": "graph create --node http://localhost:8020/ mints",
		"remove-local": "graph remove --node http://localhost:8020/ mints",
		"deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 mints"
	},
	"devDependencies": {
		"@graphprotocol/graph-cli": "0.25.2",
		"@graphprotocol/graph-ts": "0.24.1",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4",
		"mustache": "^3.1.0"
	}
}